{% extends 'base.html.twig' %}

{% block title %}All wishes | {{ parent()}} {% endblock %}


{% block body %}
    <h1>All wishes!</h1>
    <ul class="wishes">
        {% for w in wishes %}
        <li><a href="{{ path('wish_detail', {'id':w.id}) }}">{{ w.title }} [ {{ w.category ? w.category.name : "Aucune categorie" }}]</a> </li>
        {% endfor %}
    </ul>
{% endblock %}
